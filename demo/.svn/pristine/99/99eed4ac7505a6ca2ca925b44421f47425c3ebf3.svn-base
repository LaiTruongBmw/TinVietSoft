/* For Calendar */
.smcalendar  		{background-color:#B3E7FF; border: 1 solid #FFFFFF;} 
.smcalendar_table  	{border-top: 1 solid #B3E7FF; border-left: 1 solid #B3E7FF;} 
.smcalendar_header 	{background-color:#EFEFEF ; font-family: "Tahoma" ;font-weight:bold; font-size:8pt; color: #01226F; text-align: center; border-top:1 solid white ; border-left:1 solid white ; border-bottom: 1 solid #B3E7FF; border-right: 1 solid #B3E7FF}
.smcalendar_content  	{font-family: "Tahoma";color:#322E8C; text-align: center; background-color:#EFEFEF; font-size:8pt ; border-top:1 solid white ; border-left:1 solid white ; border-bottom: 1 solid #B3E7FF; border-right: 1 solid #B3E7FF;cursor:hand}
.smcalendar_last  	{font-family: "Tahoma";color:#7E7E7E; text-align: center; background-color:#EFEFEF; font-size:8pt ; border-top:1 solid white ; border-left:1 solid white ; border-bottom: 1 solid #B3E7FF; border-right: 1 solid #B3E7FF; cursor:hand}
.smcalendar_select  	{font-family: "Tahoma";color:#700035; text-weight:bold;text-align: center; background-color:#FEF68B; font-size:8pt ; border-bottom: 1 solid #B3E7FF; border-right: 1 solid #B3E7FF; cursor:hand}

/* for Main.aspx */
.tab {color: #000000; font-size:11px; cursor:hand;width:150px;padding-top:7px; text-align:center; cursor:pointer;border-right:1px solid #ffffff;}
.activeTab {color: #ffffff;font-size:11px; cursor:hand;width:150px;padding-top:7px; text-align:center;border-right:1px solid #ffffff;}
.overTab {color: #fdfcad;font-size:11px; cursor:hand;width:150px;padding-top:7px; text-align:center;border-right:1px solid #ffffff;}
.ShowAppMenuOver { background-image:none;cursor:hand;height:24;width:31;padding-top:4px; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/demo/system/menu/images/iconShowAppMenuOver.png', sizingMethod='scale');}
.ShowAppMenu { background-image:none;cursor:hand;height:24;width:31;padding-top:4px; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/demo/system/menu/images/iconShowAppMenu.png', sizingMethod='scale'); }
.ShowPin { background-image:none;cursor:hand;height:24;width:31;padding-top:4px; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/demo/system/images/pinon.png', sizingMethod='scale'); }
.ShowPinOver { background-image:none;cursor:hand;height:24;width:31;padding-top:4px; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/demo/system/images/pinon_over.png', sizingMethod='scale'); }
.ShowPinOn { background-image:none;cursor:hand;height:24;width:31;padding-top:4px; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/demo/system/images/pinoff.png', sizingMethod='scale'); }
.ShowPinOnOver { background-image:none;cursor:hand;height:24;width:31;padding-top:4px; filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='/demo/system/images/pinoff_over.png', sizingMethod='scale'); }
#footer {position:absolute;top:0px;right:10px;}
