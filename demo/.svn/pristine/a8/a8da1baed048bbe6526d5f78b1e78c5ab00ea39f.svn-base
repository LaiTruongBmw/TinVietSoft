function OnSearch()
{	
	if(check_init==1)
	{
		datAppendixContract.Call("SELECT");
	}		
        
}
//-----------------------------------------------------
function OnSave()
{
	if(check_init==1)
	{
		if(confirm((v_language =="ENG")?"Do you want to save appendix ?":"Bạn có muốn cập nhật phụ lục hợp đồng không?"))
		{
			datAppendixContract.Call();
		}
	}	
}

